{
    "name": "slowprog/maxmind-minfraud-chargeback",
    "type": "library",
    "description": "Client for Maxmind's minFraud Chargeback Web Service.",
    "keywords": [
        "minfraud-chargeback",
        "MaxMind",
        "minFraud",
        "chargeback",
        "client",
        "API",
        "fraud"
    ],
    "homepage": "https://github.com/slowprog/maxmind-minfraud-chargeback/",
    "license": "MIT",
    "authors": [
        {
            "name": "Mariano F.co Ben√≠tez Mulet",
            "email": "pachicodev@gmail.com",
            "homepage": "https://github.com/pachico",
            "role": "Developer"
        },
        {
            "name": "Andrey Tushev",
            "email": "slowprog@gmail.com",
            "homepage": "https://github.com/slowprog",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.4|~7.0",
        "php-curl-class/php-curl-class": "5.1.*",
        "webmozart/assert": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "~2.3",
        "sebastian/comparator": "1.2.4"
    },
    "autoload": {
        "psr-4": {
            "MaxMind\\MinFraudChargeback\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaxmMind\\MinFraudChargeback\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf fix ./src --standard=psr2",
        "formattests": "phpcbf fix ./tests --standard=psr2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
